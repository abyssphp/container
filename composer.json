{
    "name": "abyss/container",
    "description": "Dependency Injection, Service Container",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Dominik Szamburski",
            "email": "dominikszamburski99@gmail.com"
        }
    ],
    "require": {
        "psr/container": "^2.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "nulldark/devtools": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Abyss\\Container\\": "src/Container/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abyss\\Test\\Unit\\": "tests/unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "nulldark/devtools": true
        }
    }
}